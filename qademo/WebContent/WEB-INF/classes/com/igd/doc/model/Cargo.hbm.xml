<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.doc.model.Cargo" table="cargo">
	<id name="id"  type="java.lang.String" column="id">
			<generator class="uuid.hex"/>
	</id> 
	<property name="name" column="name" />
	<property name="category" column="category" />
	<property name="format" column="format" />
	
	<property name="medicPackage" column="medicPackage" />
	<property name="number" column="number" />
	<property name="price" column="price" />
	<property name="total" column="total" />
	<property name="lotNumber" column="lotNumber" />
	<property name="effectiveDate" column="effectiveDate" />
	<property name="medicalPermitment" column="medicalPermitment" />
	<property name="doc_id" column="doc_id" />
	</class>
	
</hibernate-mapping>