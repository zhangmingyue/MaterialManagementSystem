<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.Storeplace" table="storeplace">
	<id name="id"  type="java.lang.String" column="stp_id"  >
			<generator class="uuid.hex"/> 
	</id> 
	<property name="num" column="stp_num" />
	<property name="remark" column="remark" />

	<many-to-one name="storage"  lazy="false" class="com.igd.baseinfo.model.Storageinfo" column="store_id"   />
 
	</class>
	
</hibernate-mapping>