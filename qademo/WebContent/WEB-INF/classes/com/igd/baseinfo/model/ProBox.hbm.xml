<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.ProBox" table="probox">
		<id name="id"  type="java.lang.String" column="id">
			<generator class="uuid.hex"/>
		</id> 
		<property name="num" column="num" />
		<property name="status" column="status" />
		<property name="scrabble_batch" column="scrabble_batch" />
		<property name="scrabble_count" column="scrabble_count" />
		<many-to-one name="goodsinfo"  lazy="false" class="com.igd.baseinfo.model.Goodsinfo" column="goodsinfo_id"   />
	</class>
	
</hibernate-mapping>