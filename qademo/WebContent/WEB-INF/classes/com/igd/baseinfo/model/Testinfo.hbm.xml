<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.Testinfo" table="testinfo">
		<id name="id"  type="java.lang.String" column="t_id">
				<generator class="uuid.hex"/>
		</id> 
		<property name="num" column="test_nub" />
		<property name="tpath" column="test_path" />
		<property name="testdate" column="testdate" />
		<property name="state" column="state" />
		<property name="testcycle" column="testcycle" />
		<property name="sendate" column="sendate" />
		<property name="testres" column="testres" />
			<property name="rtcount" column="retestcount" />
		<property name="remark" column="remark" />
		<property name="testcount"   column="testcount" />
		<property name="dequalified"   column="dequalified" />
		<property name="resultinfo"   column="resultinfo" />
		<many-to-one name="goods"  lazy="false" class="com.igd.baseinfo.model.Goodsinfo" column="gi_id"   />
	</class>
	
</hibernate-mapping>