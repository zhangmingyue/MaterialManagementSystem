<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.ReTestinfo" table="retest">
		<id name="id"  type="java.lang.String" column="rt_id">
				<generator class="uuid.hex"/>
		</id> 
		<property name="testdate" column="testdate" />
		<property name="status"    column="status" />
		<property name="senddate"  column="senddate" />
		<property name="seq" column="seq" />
		<property name="test_num" column="test_num" />
		<property name="remark"   column="remark" />
		<property name="days"   column="days" />
		<property name="typecode"   column="type_code" />
		<property name="lastdate"   column="lastdate" />
		<property name="retestcount"   column="retestcount" />
		<property name="dequalified"   column="dequalified" />
		<property name="resultinfo"   column="resultinfo" />
		<many-to-one name="test"  lazy="false" class="com.igd.baseinfo.model.Testinfo" column="test_id"   />
	</class>
	
</hibernate-mapping>