<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.Goodsbase" table="goodsbase">
	<id name="id"  type="java.lang.String" column="gb_id">
			<generator class="uuid.hex"/>
	</id> 
	<property name="num" column="gb_num" />
	<property name="name" column="gb_name" />
	<property name="norms" column="gb_norms" />
	<property name="pknorms" column="packnorms" />
	<property name="pronorms" column="pronorms" />
	<property name="gmpnorms" column="gmpnorms" />
	<property name="pkunit" column="pkunit" />
	<property name="agetype" column="agetype" />
	<property name="unit" column="gb_unit" />
	<property name="cl_name" column="cl_name" />	
	<property name="cl_id" column="cl_id" />	
	<property name="spell" column="spell" />	
	<property name="status" column="status" />
	<property name="remark" column="remark" />
	<property name="cl_code" column="cl_code" />
	<property name="isauto" column="isauto" />
	<property name="medicalPermitment" column="medicalPermitment" />
	<set name="sgs" lazy="false"  inverse="true" table="supandgoods">
		<key column="gb_id"/>
		<one-to-many class="com.igd.baseinfo.model.SupAndGoods"/>
	</set>
	</class>
	
</hibernate-mapping>