<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>
	<class name="com.igd.baseinfo.model.Goodsinfo" table="goodsinfo">
	<id name="id"  type="java.lang.String" column="g_id">
			<generator class="uuid.hex"/>
	</id> 
	<property name="batch_num" column="batch_num" />
	<property name="supplier_id" column="supplier_id" />
	<property name="allcount" column="allcount" />
	<property name="curcount" column="curcount" />
	<property name="validity" column="validity" />
	<property name="factorydata" column="factorydata" />
	<property name="place" column="place" />	
	<property name="state" column="state" />
	<property name="status" column="status" />
	<property name="sp_size" column="sp_size" />
	<property name="isscrabble" column="isscrabble" />
	<property name="pervlaue" column="pervlaue" />
	<property name="supplier" column="supplier" />
	<property name="allvalue" column="allvalue" />
	<property name="remark" column="remark" />
	<property name="descount" column="descount" />
	<property name="picecount" column="picecount" />
	<property name="curpicecount" column="curpicecount" />
	<property name="gmpcount" column="gmpcount" />
	<property name="batchcount" column="batchcount" />
	<property name="fac_batch_num" column="fac_batch_num" />  
	<property name="mading" column="mading" />  
	<property name="despx" column="despx" />  
	<property name="testnum" column="testnum" />  
	<property name="lim" column="lim" />
	<property name="islock" column="islock" />
	<many-to-one name="storageInfo"  lazy="false" class="com.igd.baseinfo.model.Storageinfo" column="st_id"   />
<!-- 	<many-to-one name="testinfo"   lazy="false" class="com.igd.baseinfo.model.Testinfo" column="test_id"   /> -->
	<many-to-one name="goodsbase"  lazy="false" class="com.igd.baseinfo.model.Goodsbase" column="gb_id"   />

	</class>
	
</hibernate-mapping>