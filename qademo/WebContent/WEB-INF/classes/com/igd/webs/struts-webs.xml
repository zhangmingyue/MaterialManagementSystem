<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts>

	<package name="webs" namespace="/webs" extends="default">
		<action name="webs" class="com.igd.webs.action.WebsAction">
			<result name="news" type="redirect">/jsp/news.jsp?newsId=${newsId}&amp;columnId=${columnId}</result>
			<result name="more" type="redirect">/jsp/newslist.jsp?cid=${id}&amp;name=${name}&amp;columnId=${columnId}</result>
		</action>

	</package>
</struts>