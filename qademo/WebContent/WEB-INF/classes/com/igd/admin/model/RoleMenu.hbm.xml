<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.igd.admin.model.RoleMenu" table="sys_role_menu">
    	<id name="id" type="java.lang.String" column="s_id" unsaved-value="">
			<generator class="uuid.hex"/>
		</id>
        <property name="menuId" column="s_menu_id" />
        <property name="roleId" column="s_role_id" />

    </class>
</hibernate-mapping>
